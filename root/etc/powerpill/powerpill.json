{
    "aria2": {
        "args": [
            "--conf-path=/etc/aria2.conf"
        ],
        "path": "/usr/bin/aria2c"
    },
    "pacman": {
        "config": "/etc/pacman.conf",
        "path": "/usr/bin/pacman"
    },
    "pacserve": {
        "server": null
    },
    "powerpill": {
        "select": true,
        "reflect databases": false
    },
    "reflector": {
        "args": [
            "--protocol",
            "http",
            "--latest",
            "20",
            "--sort",
            "age"
        ]
    },
    "rsync": {
        "args": [
            "--no-motd",
            "--progress"
        ],
        "db only": true,
        "path": "/usr/bin/rsync",
        "servers": []
    }
}
